{
  "name": "@repo/jsxcord",
  "type": "module",
  "version": "0.0.1",
  "description": "Build Discord bots using React",
  "author": "PitsPower",
  "repository": {
    "type": "git",
    "url": "https://github.com/PitsPower/jsxcord.git"
  },
  "exports": {
    ".": {
      "types": "./src/index.tsx",
      "default": "./dist/index.js"
    },
    "./async": {
      "types": "./src/async.tsx",
      "default": "./dist/async.js"
    },
    "./discord": {
      "types": "./src/discord.ts",
      "default": "./dist/discord.js"
    },
    "./react": {
      "types": "./src/react.tsx",
      "default": "./dist/react.js"
    },
    "./zod": {
      "types": "./src/zod.ts",
      "default": "./dist/zod.js"
    }
  },
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsc --watch",
    "build": "typedoc src/index.tsx && tsc",
    "docs": "typedoc src/index.tsx"
  },
  "peerDependencies": {
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@discordjs/opus": "github:discordjs/opus",
    "@discordjs/voice": "^0.18.0",
    "@types/promise-queue": "^2.2.3",
    "color": "^5.0.0",
    "discord.js": "^14.16.3",
    "ffmpeg-static": "^5.2.0",
    "libsodium-wrappers": "^0.7.15",
    "prism-media": "^1.3.5",
    "promise-queue": "^2.2.5",
    "react-reconciler": "^0.28",
    "react-use-polyfill": "^0.1.0",
    "rebuild": "^0.1.2",
    "uuid": "^11.0.3",
    "zod-empty": "^1.3.4"
  },
  "devDependencies": {
    "@mxssfd/typedoc-theme": "^1.1.7",
    "@types/bun": "^1.2.2",
    "@types/react": "^18.3.12",
    "@types/react-reconciler": "^0.28",
    "globals": "^15.12.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3"
  }
}
